{
    "name": "rok_discord_bot",
    "version": "0.0.1",
    "description": "incredible bot to ask about a building path in RoK",
    "main": "index.js",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "start:dev": "nodemon src/main.ts",
        "start": "node dist/main.js",
        "deploy:commands:clean:dev": "nodemon ./src/deployments/deploy-clean-commands.ts",
        "deploy:commands:global:dev": "nodemon ./src/deployments/deploy-global-commands.ts",
        "deploy:commands:guild:dev": "nodemon ./src/deployments/deploy-guild-commands.ts",
        "deploy:commands:clean": "node dist/deployments/deploy-clean-commands.js",
        "deploy:commands:guild": "node dist/deployments/deploy-guild-commands.js",
        "deploy:commands:global": "node dist/deployments/deploy-global-commands.js",
        "deploy:commands": "npm run deploy:commands:clean && npm run deploy:commands:guild && npm run deploy:commands:global"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "eslint": "^8.57.0",
        "nodemon": "^3.1.0",
        "ts-node": "^10.9.2"
    }
}